<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="BrandView">
        <attr name="brand_type">
            <enum name="circle" value="1" />
            <enum name="rectangle" value="2" />
            <enum name="oval" value="3" />
        </attr>
        <attr name="brand_check_value" format="boolean" />
        <attr name="brand_round_value" format="integer" />
        <attr name="brand_frame_width" format="integer" />
        <attr name="brand_frame_color" format="color|reference" />
        <attr name="brand_block_value" format="color|reference" />
    </declare-styleable>

</resources>